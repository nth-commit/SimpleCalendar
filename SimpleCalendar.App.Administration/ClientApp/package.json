{
  "name": "simplecalendar-app-administration",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "auth0-js": "^9.6.1",
    "concurrently": "^3.6.0",
    "connected-react-router": "^4.3.0",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build": "build-css && build-react",
    "build-css": "node-sass-chokidar src/ -o src/",
    "build-react": "react-scripts-ts build",
    "start": "concurrently \"npm run start-css\"  \"npm run start-react\"",
    "start-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-react": "react-scripts-ts start",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/fetch-mock": "^6.0.3",
    "@types/jest": "^23.1.2",
    "@types/node": "^10.3.6",
    "@types/react": "^16.4.2",
    "@types/react-dom": "^16.0.6",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^6.0.3",
    "@types/react-router-dom": "^4.2.7",
    "@types/redux": "^3.6.0",
    "@types/webpack": "~3.8.5",
    "@types/webpack-env": "~1.13.5",
    "fetch-mock": "^6.5.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "moduleNameMapper": {
      "src(.*)$": "<rootDir>/src/$1",
      "test-helpers(.*)$": "<rootDir>/test-helpers/$1"
    }
  }
}
